{
  "input": [
        {
      "type": "v2rayGeoIPDat",
      "action": "add",
      "args": {
        "uri": "https://raw.githubusercontent.com/Loyalsoldier/geoip/release/geoip.dat",
        "onlyIPType": "ipv4"
      }
    }
  ],
  "output": [
   {
      "type": "text",
      "action": "output",
      "args":{
        "outputDir": "./publish",
        "outputName": "locneed.txt",
        "onlyIPType": "ipv4",
        "wantedList":[
          "cloudflare",
          "cloudfront",
          "facebook",
          "fastly",
          "google",
          "netflix",
          "telegram",
          "twitter"
        ]
    }
   }
  ]
}
